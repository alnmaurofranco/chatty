{
  "name": "trilha-nodejs",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev --inspect -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "prod": "yarn build && yarn start",
    "start": "NODE_ENV=production node dist/index.js",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "db:migration": "yarn typeorm migration:generate -n",
    "db:migrations": "yarn typeorm migration:create -n",
    "db:migrate": "yarn typeorm migration:run",
    "db:refresh": "yarn typeorm migration:revert",
    "db:drop": "yarn typeorm schema:drop",
    "test": "jest"
  },
  "license": "MIT",
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.5.0",
    "morgan": "^1.10.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.0.1",
    "socket.io-client": "^4.0.1",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.41",
    "@types/socket.io": "^2.1.13",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  }
}
